//                       });
                      $('a.proFeaturedImage img').each(function(){
                              jQuery(this).prop('src',$(this).prop('src').replace(/_small/g,""));
                          });
                  });
                </script>
     			
            {% endif %}
         	
        
        </main>
        {% section 'vela-footer' %}
    </div>
    {% include 'vela-ajaxcart' %}
    {% include 'vela-modal' %}
    {% include 'vela-footer-javascript' %}
	{% include 'booster-discounts' %}
  	{% if template == 'product' %}
  		<!-- start: all other pages (product, informational) -->
       <script src="https://search-nodejs.yswhosting.com/production_files/search-magic-is-livingwateraeration-shopify.min.js" id="ysw-search-magic-script-is" defer></script>
        <!-- end: all other pages (product, informational) -->
	{% endif %}
  
  {% if template != 'collection' and template != 'product'  %}
      <!-- 
    start: only search page (https://livingwateraeration.myshopify.com/search?type=product&q=pond)
    example: https://search-nodejs.yswhosting.com/stores/livingwateraeration-shopify/search-results.html
    -->
