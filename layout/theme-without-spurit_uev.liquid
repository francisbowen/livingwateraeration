  {% endif %}
  
  {% if template == 'collection' %}
  <script>
    $('#btnMenuMobile').on('click',function(){
        $('body').addClass('menuMobileActive');
    });
  	$('.close.btnMenuClose').on('click',function(){
        $('body').removeClass('menuMobileActive');
    });
  </script>
  {% endif %}
</body>
</html>
